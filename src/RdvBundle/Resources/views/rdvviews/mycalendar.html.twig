    <meta charset='utf-8' />
    <link href="{{ asset('assets/Calendrier/fullcalendar.min.css')}}" rel='stylesheet' />
    <link href="{{ asset('assets/Calendrier/fullcalendar.print.min.css')}}" rel='stylesheet' media='print' />
    <script src="{{ asset('assets/Calendrier/moment.min.js')}}"></script>
    <script src="{{ asset('assets/Calendrier/jquery.min.js')}}"></script>
    <script src="{{ asset('assets/Calendrier/fullcalendar.min.js')}}"></script>
    {%for a in rendezvous %}
    <script>

        $(document).ready(function() {

            $('#calendar').fullCalendar({
                header: {
                    left: 'prev,next today',
                    center: 'title',
                    right: 'month,agendaWeek,agendaDay,listWeek'
                },
                defaultDate: '2018-03-12',
                navLinks: true, // can click day/week names to navigate views
                editable: true,
                eventLimit: true, // allow "more" link when too many events
                events: [

                    {
                        title: '{{ a.etat }}',
                        url:'www.google.com',
                        start: '{{ a.date }}'
                    }
                ]
            });

        });

    </script>
    {% endfor %}
    <style>

        body {
            margin: 40px 10px;
            padding: 0;
            font-family: "Lucida Grande",Helvetica,Arial,Verdana,sans-serif;
            font-size: 14px;
        }

        #calendar {
            max-width: 900px;
            margin: 0 auto;
        }

    </style>
</head>
<body>

<div id='calendar'></div>

</body>
</html>





